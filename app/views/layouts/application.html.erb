<!DOCTYPE html>
<html>
<head>
  <title>AuthExample</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	
<div id="flash">
  <% flash.each do |key, value| -%>
    <div id="flash_<%= key %>" class="<%= key %>"><%= value %></div>
  <% end -%>
</div>

<%= yield %>

</body>
</html>
